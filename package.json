{
  "name": "ncu-report",
  "version": "0.1.0",
  "private": true,
  "description": "使用 TypeScript，未使用任何 UI 库。",
  "repository": {
    "type": "git",
    "url": "https://github.com/alibaba-fusion/materials/tree/master/scaffolds/scaffold-simple"
  },
  "scripts": {
    "build": "icejs build",
    "build:analyzer": "mincud 'icejs build --analyzer'",
    "dev": "icejs start",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "lint": "npm run eslint && npm run stylelint",
    "start": "mincud 'icejs start'",
    "start:analyzer": "mincud 'icejs start --analyzer'",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "stylelint:fix": "npm run stylelint -- --fix"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "ga-lite": "^2.1.0",
    "lodash.get": "^4.4.2",
    "mincu-react": "^2.0.0-alpha.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-spring": "^9.3.2"
  },
  "devDependencies": {
    "@iceworks/spec": "^1.0.0",
    "@types/google.analytics": "^0.0.40",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "eslint": "^7.0.0",
    "ice.js": "2.1.2",
    "mincu-debug": "2.2.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "iceworks": {
    "type": "react",
    "adapter": "adapter-react-v3"
  },
  "ideMode": {
    "name": "ice-react"
  },
  "originTemplate": "@alifd/scaffold-simple",
  "proxyConfig": {
    "/": {
      "enable": true,
      "target": "http://127.0.0.1:3333"
    }
  }
}
