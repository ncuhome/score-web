{
  "name": "ncu-report",
  "version": "0.1.0",
  "description": "使用 TypeScript，未使用任何 UI 库。",
  "devDependencies": {
    "@iceworks/spec": "^1.0.0",
    "@types/google.analytics": "^0.0.40",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "eslint": "^6.8.0",
    "ice.js": "2.0.0-rc.3",
    "mincu-debug": "2.1.0-alpha.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-prettier": "^1.2.0"
  },
  "scripts": {
    "start": "mincud 'icejs start'",
    "start:analyzer": "mincud 'icejs start --analyzer'",
    "build": "icejs build",
    "build:analyzer": "mincud 'icejs build --analyzer'",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint --cache --ext .js,.jsx,.ts,.tsx ./",
    "eslint:fix": "npm run eslint -- --fix",
    "stylelint": "stylelint \"**/*.{css,scss,less}\"",
    "stylelint:fix": "npm run stylelint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alibaba-fusion/materials/tree/master/scaffolds/scaffold-simple"
  },
  "ideMode": {
    "name": "ice-react"
  },
  "iceworks": {
    "type": "react",
    "adapter": "adapter-react-v3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "private": true,
  "originTemplate": "@alifd/scaffold-simple",
  "dependencies": {
    "ahooks": "^2.10.8",
    "antd-mobile": "^2.3.4",
    "axios": "^0.21.1",
    "blueimp-md5": "^2.18.0",
    "build-plugin-antd": "^0.1.1",
    "clsx": "^1.1.1",
    "ga-lite": "^2.1.0",
    "ice": "^3.7.60",
    "lodash.get": "^4.4.2",
    "mincu-react": "^2.0.0-alpha.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-useportal": "^1.0.14"
  },
  "proxyConfig": {
    "/": {
      "enable": true,
      "target": "http://127.0.0.1:3333"
    }
  }
}
